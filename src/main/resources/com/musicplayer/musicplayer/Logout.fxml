<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.musicplayer.musicplayer.Logout">

    <MenuButton mnemonicParsing="false" prefHeight="32.0" prefWidth="90.0" style="-fx-background-color: none;" styleClass="frameUser" stylesheets="@Style.css" text="User" textFill="#f2eaea" >
        <font>
            <Font name="Arial Rounded MT Bold" size="11.0" />
        </font>
        <graphic>
            <ImageView fitHeight="24.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@images/AvtUser.png" />
                </image>
                <effect>
                    <Shadow height="0.0" radius="0.0" width="0.0">
                        <color>
                            <Color red="0.7263157963752747" green="0.6886549592018127" blue="0.6886549592018127" />
                        </color>
                    </Shadow>
                </effect>
            </ImageView>
        </graphic>
        <items>
            <MenuItem mnemonicParsing="false" onAction="#logoutAction" text="Logout" />
        </items>
    </MenuButton>

</AnchorPane>
